optional-dependencies:
    - buttonfacade
    - omni-cc
    - tullacc
    - libdatabroker-1-1

manual-changelog:
    filename: CHANGELOG.txt
    markup-type: creole

license-output: LICENSE.txt
enable-nolib-creation: yes

move-folders:
    TellMeWhen/TellMeWhen_Options: TellMeWhen_Options

ignore:
    - scrapper.lua
    - USEDLOCALE.lua
    - Options/scrapper.lua
    - Options/USEDLOCALE.lua
    - Lib
    - Scripts
    - Documentation
    - README.md
    - CHANGELOG.txt
